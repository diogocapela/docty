                android:layout_marginRight="@dimen/list_margin_sides">

                <TextView
                    android:id="@+id/hormones_prl_display"
                    android:text="@string/hormones_prl"
                    android:layout_below="@+id/hormones_prl_title"
                    android:layout_width="match_parent"
