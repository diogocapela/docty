                    android:text="@string/hormones_afp"
                    android:layout_below="@+id/hormones_afp_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/display_font_size"
                    android:textColor="@color/display_color" />
                <TextView
                    android:id="@+id/hormones_afp_unit"
                    android:text="@string/hormones_afp_unit"
                    android:layout_width="wrap_content"
                    android:layout_height="?attr/listPreferredItemHeight"
